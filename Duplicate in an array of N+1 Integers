public class Duplicate {
    public static void main(String[] args) {
        int arr[] =  {3,1,4,4,2};
        int size=arr.length-1;
        int add=(size*(size+1))/2,sum=0;
        for(int i=0;i<arr.length;i++){
            sum=sum+arr[i];
        }
        System.out.println("repeated element is:"+(sum-add));
    }
    
}
